<view class="container">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="true" easing-function="easeInCubic">
    <block wx:for="{{background}}" wx:key="*this" >
      <swiper-item>
        <view class="swiper-item {{item}}"><image src="{{background[index].file}}" /></view>
      </swiper-item>
    </block>
  </swiper>
</view>